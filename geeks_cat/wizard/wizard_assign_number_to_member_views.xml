<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="wizard_assing_number_to_member_form" model="ir.ui.view">
        <field name='name'>wizard.assign.number.to.member.form</field>
        <field name='model'>wizard.assign.number.to.member</field>
        <field name='arch' type='xml'>
            <form string='Assign number to member'>
                <button name='assign_number_to_member' string='Assign'
                    type='object'  class='oe_highlight' />
                <button special="cancel" string="Cancel" type="object"/>
            </form>
        </field>
    </record>
    <act_window
            id="action_assing_number_to_member"
            res_model="wizard.assign.number.to.member"
            binding_model="res.partner"
            target="new"
            view_mode="form"
            name="Assign number to member"/>

</odoo>
